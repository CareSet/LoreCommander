</tr></tfoot>
<tbody>
  {{#data}}
    <tr>
	<th scope='row'><a href='/DURC/orderdetail/{{id}}/'> {{id}} </a></th>			<td>{{order_id_DURClabel}} <a href='/DURC/order/{{order_id}}/'> ({{order_id}})</a> </td>			<td>{{product_id_DURClabel}} <a href='/DURC/product/{{product_id}}/'> ({{product_id}})</a> </td>			<td>{{quantity}}</td>
			<td>{{unitPrice}}</td>
			<td>{{discount}}</td>
			<td>{{dateAllocated}}</td>
			<td>{{PurchaseOrder_id_DURClabel}} <a href='/DURC/purchaseorder/{{PurchaseOrder_id}}/'> ({{PurchaseOrder_id}})</a> </td>
    </tr>
  {{/data}}

<tbody></table>



{{^is_need_paging}}
<p>
