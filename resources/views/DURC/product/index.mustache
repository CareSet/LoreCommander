{{#is_need_paging}}

<div class='dataTables_paginate paging_simple_numbers' id='table_product_page'>
	<ul class='pagination'>
		<li class='paginate_button page-item previous {{first_page_class}}' 
			id='table_product_previous'>
			<a href='{{first_page_url}}' aria-controls='table_product' data-dt-idx='0' tabindex='0' class='page-link'>First</a>
		</li>
