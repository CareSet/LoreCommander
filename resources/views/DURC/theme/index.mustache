
<h1>theme list </h1>
<h3> <a href='/Zermelo/DURC_theme'>Zermelo theme Report</a></h3>
Create <a href='/DURC/theme/create/'>new theme</a><br>


{{^is_need_paging}}
<p>
{{total}} results. All rows shown.
</p>
{{/is_need_paging}}

{{#is_need_paging}}

<div class='dataTables_paginate paging_simple_numbers' id='table_theme_page'>
	<ul class='pagination'>
		<li class='paginate_button page-item previous {{first_page_class}}' 
			id='table_theme_previous'>
			<a href='{{first_page_url}}' aria-controls='table_theme' data-dt-idx='0' tabindex='0' class='page-link'>First</a>
		</li>
		<li class='paginate_button page-item {{prev_page_class}}'>
			<a href='{{prev_page_url}}' aria-controls='table_theme' data-dt-idx='1' tabindex='0' class='page-link'>Previous</a>
		</li>
		<li class='paginate_button page-item active'>
			<a href='#' aria-controls='table_theme' data-dt-idx='2' tabindex='0' class='page-link'>{{current_page}}</a>
		</li>
		<li class='paginate_button page-item {{next_page_class}} '>
			<a href='{{next_page_url}}' aria-controls='table_theme' data-dt-idx='3' tabindex='0' class='page-link'>Next</a>
		</li>
		<li class='paginate_button page-item {{last_page_class}}'>
			<a href='{{last_page_url}}' aria-controls='table_theme' data-dt-idx='4' tabindex='0' class='page-link'>Last</a>
		</li>
	</ul>
</div>
{{/is_need_paging}}


<table id='table_theme' class='table table-bordered table-hover table-responsive table-sm'>
<thead><tr>
<th scope='col'>#</th><th scope='col'>theme_name</th>
<th scope='col'>theme_description</th>
<th scope='col'>emblematic_person_id</th>
<th scope='col'>emblematic_cardface_id</th>
<th scope='col'>emblematic_creature_id</th>
<th scope='col'>created_at</th>
<th scope='col'>updated_at</th>

</tr></thead>
<tfoot><tr>
<th scope='col'>#</th><th scope='col'>theme_name</th>
<th scope='col'>theme_description</th>
<th scope='col'>emblematic_person_id</th>
<th scope='col'>emblematic_cardface_id</th>
<th scope='col'>emblematic_creature_id</th>
<th scope='col'>created_at</th>
<th scope='col'>updated_at</th>

</tr></tfoot>
<tbody>
  {{#data}}
    <tr>
	<th scope='row'><a href='/DURC/theme/{{id}}/'> {{id}} </a></th>			<td>{{theme_name}}</td>
			<td>{{theme_description}}</td>
			<td>{{emblematic_person_id_DURClabel}} <a href='/DURC/person/{{emblematic_person_id}}/'> ({{emblematic_person_id}})</a> </td>			<td>{{emblematic_cardface_id_DURClabel}} <a href='/DURC/cardface/{{emblematic_cardface_id}}/'> ({{emblematic_cardface_id}})</a> </td>			<td>{{emblematic_creature_id_DURClabel}} <a href='/DURC/creature/{{emblematic_creature_id}}/'> ({{emblematic_creature_id}})</a> </td>			<td>{{created_at}}</td>
			<td>{{updated_at}}</td>

    </tr>
  {{/data}}

<tbody></table>



{{^is_need_paging}}
<p>
{{total}} results. All rows shown.
</p>
{{/is_need_paging}}

{{#is_need_paging}}

<div class='dataTables_paginate paging_simple_numbers' id='table_theme_page'>
	<ul class='pagination'>
		<li class='paginate_button page-item previous {{first_page_class}}' 
			id='table_theme_previous'>
			<a href='{{first_page_url}}' aria-controls='table_theme' data-dt-idx='0' tabindex='0' class='page-link'>First</a>
		</li>
		<li class='paginate_button page-item {{prev_page_class}}'>
			<a href='{{prev_page_url}}' aria-controls='table_theme' data-dt-idx='1' tabindex='0' class='page-link'>Previous</a>
		</li>
		<li class='paginate_button page-item active'>
			<a href='#' aria-controls='table_theme' data-dt-idx='2' tabindex='0' class='page-link'>{{current_page}}</a>
		</li>
		<li class='paginate_button page-item {{next_page_class}} '>
			<a href='{{next_page_url}}' aria-controls='table_theme' data-dt-idx='3' tabindex='0' class='page-link'>Next</a>
		</li>
		<li class='paginate_button page-item {{last_page_class}}'>
			<a href='{{last_page_url}}' aria-controls='table_theme' data-dt-idx='4' tabindex='0' class='page-link'>Last</a>
		</li>
	</ul>
</div>
{{/is_need_paging}}


<script type='text/javascript'>
//we need a way to check to see if the page is loaded before we call DataTable()
//but we frequently do not have JQuery yet... it could be loaded at the bottom of the page...
//so we have a pure JS alternative to the ready() function..
var DURC_checkReadyState = setInterval(() => {
  if (document.readyState === 'complete') {
    clearInterval(DURC_checkReadyState);
    // document is ready
    $('#table_theme').DataTable(
{
        'paging':   false,
        'info':     false
}
);
  }
}, 100);


</script>

